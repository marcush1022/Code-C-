/*********************************************************************************/
1. 求N!末尾0的数量
2. N!末尾最低位1的位置
/*********************************************************************************/

//Location of last 1 of the factorial in binary
int getLastOne(int n)
{
	int count=0;
	while(n)
	{
		count+=n>>1;
		n>>=1;
	}
	return count+1;
}

//Number of zero at the end of the factorial
int getNumberOfZero(int n)
{
	int count=0;
	while(n)
	{
		count+=n/5;
		n/=5;
	}
	return count;
}

int main()
{
	cout<<getLastOne(4)<<endl;
	cout<<getNumberOfZero(10);
	return 0;
}
