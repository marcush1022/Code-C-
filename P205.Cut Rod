
class Solution {
public:
    	void cutRod(vector<int> &P, int len, vector<int> &r)
	{
		int maxBefore;
		//int r[len];
		r.resize(len+1);
		r[0]=0;
		for(int i=1; i<=len; i++)
		{
			maxBefore=-1;
			for(int j=1; j<=i; j++)
			{
				maxBefore=max(maxBefore, (P[j]+r[i-j]));
			}
			r[i]=maxBefore;
		}
	}
};

int main()
{
    Solution s;
    int p[] = {-1,1,5,8,9,10,17,17,20,24,30};
    vector<int> vp(p,p + sizeof(p)/sizeof(int));
    vector<int> result;
    s.cutRod(vp,9,result);
    for(int i = 1; i <=9; ++i)
        cout << result[i] << " ";
}
